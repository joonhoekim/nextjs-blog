{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "cross-env cpy './node_modules/primereact/resources/**/*' './public/primereact/resources' --parents",
    "dev": "env-cmd -f ./nextjs-blog-submodule/.env next dev",
    "build": "env-cmd -f ./nextjs-blog-submodule/.env next build",
    "start": "env-cmd -f ./nextjs-blog-submodule/.env next start",
    "lint": "env-cmd -f ./nextjs-blog-submodule/.env next lint",
    "db:generate": "env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma generate",
    "db:push": "env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma db push",
    "db:studio": "env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma studio",
    "db:seed": "env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma db seed",
    "db:sync": "env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma generate && env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma db push",
    "db:reset": "env-cmd -f ./nextjs-blog-submodule/.env npm exec prisma db push --force-reset && env-cmd -f ./nextjs-blog-submodule/.env prisma db seed"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.3",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@lexical/react": "^0.20.0",
    "@lexical/rich-text": "^0.20.0",
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.4",
    "react": "^18.3.1",
    "react-arborist": "^3.4.0",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
